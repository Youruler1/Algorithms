# brute force

def fibonacci(num: int) -> int :
    if(n == 0):
        return 0
    if(n == 1):
        return 1
    return fibonacci(num-1) + fibonacci(num-2)

if __name__ == "__main__":
    num = int(input("Enter the number to take fibonacci of: "))
    print(f"\nFibonacci of {n} is: {fibonacci(n)}")